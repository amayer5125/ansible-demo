{{ ansible_managed | comment }}

APP_NAME="{{ laravel_app.name }}"
APP_ENV={{ laravel_app.env }}
APP_KEY=base64:{{ laravel_app.key }}
APP_URL={{ laravel_app.url }}
{% if laravel_app.debug is defined and laravel_app.debug %}
APP_DEBUG=true
{% endif %}

DB_CONNECTION={{ laravel_db.connection }}
DB_HOST={{ laravel_db.host }}
DB_PORT={{ laravel_db.port }}
DB_DATABASE={{ laravel_db.database }}
DB_USERNAME={{ laravel_db.username }}
DB_PASSWORD="{{ laravel_db.password }}"
